<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>com.donna.app</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>Donna</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>0.1.0</string>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>LSMinimumSystemVersion</key>
	<string>13.0</string>
	<key>NSHighResolutionCapable</key>
	<true/>
	<key>NSPrincipalClass</key>
	<string>NSApplication</string>
    <key>NSAppleEventsUsageDescription</key>
    <string>Donna needs access to Apple Events to integrate with system apps like Messages, Notes, Mail, and Calendar.</string>
    <key>NSContactsUsageDescription</key>
    <string>Donna needs access to your Contacts to search and manage them.</string>
    <key>NSCalendarsUsageDescription</key>
    <string>Donna needs access to your Calendar to manage events and appointments.</string>
    <key>NSRemindersUsageDescription</key>
    <string>Donna needs access to your Reminders to create and manage tasks.</string>
    <key>NSAppleScriptEnabled</key>
    <true/>
</dict>
</plist>
